{
  "manifest_version": 2,
  "name": "I N J E C T   G R I D",
  "version": "1.0",
  "description": "",
  "icons": {
    "48": "icons/bgpicker-48.png"
  },
  "permissions": [
    "tabs",
    "cookies",
    "<all_urls>"
  ],
  "browser_action": {
    "default_icon": {
      "32": "icons/bgpicker-32.png"
    },
    "default_title": "I N J E C T   G R I D",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background_scripts/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content_scripts/inject.js"
      ]
    }
  ],
  "content_security_policy": "script-src *"
}
